package sub3;

import java.util.ArrayList;
import java.util.List;

/**
 * 날짜 : 2023/06/28
 * 이름 : 김상엽
 * 내용 : Ch07. List 실습하기
 * 
 */
public class ListTest {
	
	public static void main(String[] args) {
		
		// List 생성
		List<Integer> list = new ArrayList<>();
		
		// 데이터 입력
		list.add(1);
		list.add(2);
		list.add(3);
		list.add(4);
		list.add(5);
		list.add(1,6);
		
		System.out.println(list);
		
	}
}
