public class RegularExpressionTest {
	
	public static void main(String[] args) {
		//정규표현식
		String strPatt = "a[a-z]*";
		boolean r1=Pattern.matches(strPatt, "apPP");
		System.out.println(r1);
		//정규표현식 패턴
		//자주사용하는 정규표현식
		
		
	}

}
