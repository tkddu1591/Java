public class ThrowsTest {
	
	public static void myMethod1(int n1, int n2) {
		myMethod2(n1, n2);
		
	}
	public static void myMethod2(int n1, int n2)throws ArithmeticException {
		int result = n1/n2;
		System.out.println("result : "+result);
	}
	
	public static void main(String[] args) {
		
	}
	
}
